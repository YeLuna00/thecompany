                    <<= '<img class="bg" src="' + _bg + '">'>>

                    <<if _actors is 1>>
                        /* Draw Actor1 */
                        <<= '<img class="a' + _zoom + 'z" src="' + _body1 + '">'>>
                    <<elseif _actors is 2>>
                        /* Draw Actor 1 */
                        <<= '<img class="a1" src="' + _body1 + '">'>>
                        
                        /* Draw Actor 2 */
                        <<= '<img class="a2" src="' + _body2 + '">'>>
                    <</if>>
                </div>
            <</if>>
        <<else>>
            /* If the first argument is a string */
            <<set _a = $args[0]>>
			<<set _img = "pics/" + _a + ".jpg">>
        
            /* Display the image file */
            @@.pic;[img[_img]]@@
        <</if>>
    <</if>>
