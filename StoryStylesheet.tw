	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#546460', endColorstr='#2a3d3d',GradientType=0);
	background-color:#546460;
	-moz-border-radius:6px;
	-webkit-border-radius:6px;
	border-radius:6px;
	border:1px solid #ffffff;
