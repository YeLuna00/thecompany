}
  
.videoFrame {
	display: block;
	margin: auto;
	width: 85%;
	padding: 1px;
	border: 2px solid white;
	border-radius: 5px;
}

#charFull {
	position: relative;
	display: flex;
	justify-content: center;
	width: 95%;
  	height: 600px;
	padding: 5px;
	overflow: hidden;
}
#charFull img {
 	position: absolute;
	height: 100%;
}
#charFull .pa1 {
	top: 0;
  	height: 100%;
}

#charPort {
	position: relative;
	display: flex;
	justify-content: center;
	width: 95%;
  	height: 200px;
	padding: 5px;
	overflow: hidden;
  	border: 2px solid white;
  	border-radius: 5px;
}
#charPort img {
 	position: absolute;
	height: 100%;
}
#charPort .pa1 {
	/* top: -5%; */
  	/* left: -60%; */
	top: 0;
  	height: 225%;
}

#charThumb {
	position: relative;
  	display: flex;
	justify-content: center;
  	padding: 1px;
 	height: 84px;
	width: 84px;
  	float: left;
	overflow: hidden;
  	margin: 0px 10px 0px 0px;
  	border: 2px solid white;
  	border-radius: 5px;
}
#charThumb img {
 	position: absolute;
	height: 100%;
}
#charThumb .pa1 {
	top: 0;
  	height: 225%;
}

#scene {
	position: relative;
	display: flex;
	justify-content: center;
	width: 100%;
  	height: 400px;
	padding: 0px;
	overflow: hidden;
	border: 2px solid white;
	border-radius: 5px;
}
#scene img {
 	position: absolute;
	height: 100%;
}
#scene .bg {
	left: 0;
	top: 0;
  	width: 100%;
}

/* Single actor zoom levels 0,1,2 */
#scene .a0z {
	top: 20;
}
#scene .a1z {
	top: 20;
	height: 150%;
}
#scene .a2z {
	top: 20;
	height: 200%;
}

/* Dual actors on screen */
#scene .a1 {
	left: 15%;
	top: 20;
}
#scene .a2 {
	right: 15%;
	top: 20;
}

/* Player Position */
#scene .pa1 {
	left: 10%;
	top: -30px;
  	height: 175%;
}

#sceneMobile {
	position: relative;
	display: flex;
	justify-content: center;
	width: 95%;
  	height: 200px;
	padding: 5px;
	overflow: hidden;
	border: 2px solid white;
	border-radius: 5px;
}
#sceneMobile img {
 	position: absolute;
	height: 100%;
}
#sceneMobile .bg {
	left: 0;
	top: 0;
  	width: 100%;
}
#sceneMobile .a1 {
	/*height: 100%;*/
	left: 5%;
	top: 20;
}
#sceneMobile .a2 {
	/*height: 100%;*/
	right: 5%;
	top: 20;
}
#sceneMobile .a0z {
	top: 20;
}
#sceneMobile .a1z {
	height: 150%;
	top: 20;
}
#sceneMobile .a2z {
	height: 200%;
	top: 20;
}

