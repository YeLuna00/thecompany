#menu-core {
	display: none;
}
table, td {
    border: 1px solid white;
}
th {
	text-align: center;
	font-weight: bold;
    background-color: midnightblue;
	border-bottom: 1px solid white;  
}
td, th {
  	padding: 5px;
}  
td {
	text-align: left;
}
.hide {
	display: none;
}

.row {
	display: flex;
}

.column-50 {
	flex: 50%;
	padding: 10px;
}

.column-25 {
	flex: 25%;
	padding: 10px;
}

.column-75 {
	flex: 75%;
	padding: 10px;
}

.column-33 {
	flex: 33%;
	padding: 10px;
}

.column-66 {
	flex: 66%;
	padding: 10px;
}

table#bars {
	width: 100%;
	border: 1px solid white;
	padding: 5px;
	background-color: #151524;
	text-align: center;
}
table#bars th, td, tr {
	border: none;
	border-right: 1px solid white;
	padding: 5px;
	background-color: #111119;
	text-align: center;
}

img {

}
.pic > img {
	display: block;
    margin: auto;
    width: 85%;
  	padding: 1px;
  	border: 2px solid white;
  	border-radius: 5px;
}
.picNB > img {
	display: block;
    margin: auto;
    width: 95%;
